<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moosecat.block12" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <!-- theirs -->
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <!-- ours -->
    <name>bloq</name>
    <description>
       get to level 12 or you're a stupid idiot
    </description>
    <author email="richardsjustind@gmail.com" href="github.com/magentanova">
      moosecat 
    </author>
    <platform name="ios">
             <!-- iOS 8.0+ -->
             <!-- iPhone 6 Plus  -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-60@3x.png" width="180" height="180" />
             <!-- iOS 7.0+ -->
             <!-- iPhone / iPod Touch  -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-60.png" width="60" height="60" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-60@2x.png" width="120" height="120" />
             <!-- iPad -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-76.png" width="76" height="76" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-76@2x.png" width="152" height="152" />
             <!-- iOS 6.1 -->
             <!-- Spotlight Icon -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-40.png" width="40" height="40" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-40@2x.png" width="80" height="80" />
             <!-- iPhone / iPod Touch -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon.png" width="57" height="57" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon@2x.png" width="114" height="114" />
             <!-- iPad -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-72.png" width="72" height="72" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-72@2x.png" width="144" height="144" />
             <!-- iPhone Spotlight and Settings Icon -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-Small.png" width="29" height="29" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-Small@2x.png" width="58" height="58" />
             <!-- iPad Spotlight and Settings Icon -->
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-Small-50.png" width="50" height="50" />
             <icon src="apple-icon/ios/AppIcon.appiconset/Icon-Small-50@2x.png" width="100" height="100" />
    </platform>
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
</widget>
