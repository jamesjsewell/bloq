<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moosecat.block12" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <name>bloq</name>
    <description>
       get to level 12 or you're a stupid idiot
    </description>
    <author email="richardsjustind@gmail.com" href="github.com/magentanova">
      moosecat 
    </author>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="apple-icon/ios/AppIcon.appiconset/Icon-60@3x.png" width="180" />
    <icon height="60" src="apple-icon/ios/AppIcon.appiconset/Icon-60.png" width="60" />
    <icon height="120" src="apple-icon/ios/AppIcon.appiconset/Icon-60@2x.png" width="120" />
    <icon height="76" src="apple-icon/ios/AppIcon.appiconset/Icon-76.png" width="76" />
    <icon height="152" src="apple-icon/ios/AppIcon.appiconset/Icon-76@2x.png" width="152" />
    <icon height="40" src="apple-icon/ios/AppIcon.appiconset/Icon-40.png" width="40" />
    <icon height="80" src="apple-icon/ios/AppIcon.appiconset/Icon-40@2x.png" width="80" />
    <icon height="57" src="apple-icon/ios/AppIcon.appiconset/Icon.png" width="57" />
    <icon height="114" src="apple-icon/ios/AppIcon.appiconset/Icon@2x.png" width="114" />
    <icon height="72" src="apple-icon/ios/AppIcon.appiconset/Icon-72.png" width="72" />
    <icon height="144" src="apple-icon/ios/AppIcon.appiconset/Icon-72@2x.png" width="144" />
    <icon height="29" src="apple-icon/ios/AppIcon.appiconset/Icon-Small.png" width="29" />
    <icon height="58" src="apple-icon/ios/AppIcon.appiconset/Icon-Small@2x.png" width="58" />
    <icon height="50" src="apple-icon/ios/AppIcon.appiconset/Icon-Small-50.png" width="50" />
    <icon height="100" src="apple-icon/ios/AppIcon.appiconset/Icon-Small-50@2x.png" width="100" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
</widget>
